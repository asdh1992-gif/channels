    ๐ ุชู ุงูุฅูุดุงุก ุจูุงุณุทุฉ ุฏููู ุงููููุงุช ุงูุชุนููููุฉ ููุฃุทูุงู | ุฌููุน ุงูุญููู ูุญููุธุฉ ยฉ 2025
  </footer>

  <div class="toast" id="toast">ุชู ุงููุณุฎ โ</div>

  <script>
    // ===== Toast + Copy =====
    const toast = document.getElementById('toast');

    function showToast(message){
      toast.textContent = message || 'ุชู ุงููุณุฎ โ';
      toast.classList.add('show');
      setTimeout(()=>toast.classList.remove('show'), 1800);
    }

    function copyText(txt){
      if(navigator.clipboard && window.isSecureContext){
        navigator.clipboard.writeText(txt).then(()=>showToast('ุชู ุงููุณุฎ โ'));
      }else{
        const ta=document.createElement('textarea');
        ta.value=txt; document.body.appendChild(ta);
        ta.select(); ta.setSelectionRange(0,99999);
        try{ document.execCommand('copy'); }catch(e){}
        ta.remove(); showToast('ุชู ุงููุณุฎ โ');
      }
    }

    document.querySelectorAll('.btn.copy').forEach(btn=>{
      btn.addEventListener('click', ()=>{
        const url = (btn.dataset.url || '').trim();
        if(!url || url.toLowerCase() === 'nan'){
          showToast('ูุง ููุฌุฏ ุฑุงุจุท ููุฐู ุงูููุงุฉ');
          return;
        }
        copyText(url);
      });
    });

    // ===== Filters + Search =====
    function normalizeAge(s){
      return (s || '').replace(/\s*ุณููุงุช?\s*/g,'').trim();
    }

    const typeFilter  = document.getElementById("typeFilter");
    const ageFilter   = document.getElementById("ageFilter");
    const langFilter  = document.getElementById("langFilter");
    const searchInput = document.getElementById("searchInput");

    function filterCards(){
      const selectedType = typeFilter.value;
      const selectedAge  = ageFilter.value;
      const selectedLang = langFilter.value;
      const q = (searchInput.value || "").trim().toLowerCase();

      document.querySelectorAll(".card").forEach(card=>{
        const type  = card.dataset.type || "";
        const age   = card.dataset.age  || "";
        const lang  = card.dataset.lang || "";
        const title = (card.querySelector(".title")?.textContent || "").toLowerCase();

        const matchFilters =
          (selectedType === "" || type === selectedType) &&
          (selectedLang === "" || lang === selectedLang) &&
          (selectedAge === ""  || normalizeAge(age) === normalizeAge(selectedAge));

        const matchSearch = (q === "" || title.includes(q));

        card.style.display = (matchFilters && matchSearch) ? "flex" : "none";
      });
    }

    typeFilter.addEventListener("change", filterCards);
    ageFilter.addEventListener("change", filterCards);
    langFilter.addEventListener("change", filterCards);
    searchInput.addEventListener("input", filterCards);

    // ุชุดุบูู ุฃููู
    filterCards();
  </script>
